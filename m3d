toilet -f slant --gay "    M3D"
php phd.php | lolcat
exit
fi

if [ $med = 8 ] || [ $med = 08 ]
then
cd module
clear
toilet -f slant --gay "    M3D"
php whiskas.php | lolcat
exit
fi

if [ $med = 9 ] || [ $med = 09 ]
then
cd module
clear
toilet -f slant --gay "    M3D"
php zipay.php | lolcat
exit
fi

if [ $med = 10 ]
then
cd module
