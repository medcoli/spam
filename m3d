then
cd module
clear
toilet -f slant --gay "    M3D"
php hooq.php | lolcat
exit
fi

if [ $med = 6 ] || [ $med = 06 ]
then
cd module
clear
toilet -f slant --gay "   M3D"
php kfc.php | lolcat
exit
fi

